PROJECT_NAME=Avium
PROJECT_NUMBER=@CMAKE_PROJECT_VERSION@
PROJECT_BRIEF=@CMAKE_PROJECT_DESCRIPTION@
OUTPUT_DIRECTORY=@AVM_DOC_OUTPUT_DIR@
ALLOW_UNICODE_NAMES=YES
JAVADOC_AUTOBRIEF=YES
JAVADOC_BANNER=YES
# QT_AUTOBRIEF=YES
TAB_SIZE=@AVM_TAB_WIDTH@
OPTIMIZE_OUTPUT_FOR_C=YES
TYPEDEF_HIDES_STRUCT=YES
EXTRACT_ALL=YES
EXTRACT_STATIC=YES
WARN_IF_UNDOCUMENTED=YES
RECURSIVE=YES
INPUT=@AVM_INCLUDE_DIR@
GENERATE_HTML=NO
GENERATE_LATEX=NO
GENERATE_XML=YES
GENERATE_MAN=YES
INCLUDE_PATH=@AVM_INCLUDE_OUTPUT_DIR@/avium @AVM_INCLUDE_DIR@/avium
EXCLUDE=@AVM_INCLUDE_DIR@/avium/config.h
